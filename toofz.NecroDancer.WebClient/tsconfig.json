{
  "compilerOptions": {
    "alwaysStrict": true,
    "importHelpers": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": [
      "DOM",
      "ES5",
      "ScriptHost",
      "ES2015.Promise"
    ]
  },
  "exclude": [
    "node_modules",
    "gulpfile.ts"
  ]
}